-- StarterGui.sigmaGui.Frame.UIStroke.UIGradient.LocalScript
local function C_6()
local script = G2L["6"];
	local uiGradient = script.Parent
	
	while true do
		for i = 0, 360, 1 do
			uiGradient.Rotation = i
			wait(0.01)
		end
	end
	
end;
task.spawn(C_6);
-- StarterGui.sigmaGui.Frame.TextLabel.UIGradient.LocalScript
