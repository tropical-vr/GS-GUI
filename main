-- StarterGui.sigmaGui.Frame.LocalScript
local function C_29()
local script = G2L["29"];
	local audioId = "rbxassetid://8486683243"
	local sound = Instance.new("Sound")
	sound.SoundId = audioId
	sound.Parent = game.Workspace
	
	while true do
		if sound.IsLoaded then
			sound:Play()
			break
		end
		wait(0.1)
	end
	
end;
task.spawn(C_29);
-- StarterGui.sigmaGui.sigma.ImageButton.LocalScript
local function C_2c()
local script = G2L["2c"];
	local button = script.Parent
	local frame = script.Parent.Parent.Parent.Frame
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://4499400560"
	sound.Parent = game.Workspace
	
	button.MouseButton1Click:Connect(function()
		if frame.Visible then
			frame.Visible = false
		else
			frame.Visible = true
		end
		sound:Play()
	end)
	
end;
task.spawn(C_2c);

